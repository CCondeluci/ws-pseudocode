{
  "name": "ws-sim-syntax",
  "displayName": "ws-sim-syntax",
  "description": "VSCode syntax highlighting for CardDate files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "WSCD",
      "aliases": ["WS Card Data Psuedocode", "WSCD"],
      "extensions": ["txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "WSCD",
      "scopeName": "source.wscd",
      "path": "./syntaxes/WSCD.tmLanguage.json"
    }]
  }
}
